import com.popstack.mvoter2015.domain.news.model.NewsId;
import java.lang.Boolean;

CREATE TABLE NewsSearchRemoteKeyTable(
    newsId TEXT AS NewsId PRIMARY KEY NOT NULL,
    previousKey INTEGER,
    nextKey INTEGER
);

insertOrReplace:
INSERT OR REPLACE INTO NewsSearchRemoteKeyTable VALUES (
    :newsId, :previousKey, :nextKey
);

selectById:
SELECT * FROM NewsSearchRemoteKeyTable WHERE newsId = :id;

deleteAll:
DELETE FROM NewsSearchRemoteKeyTable;