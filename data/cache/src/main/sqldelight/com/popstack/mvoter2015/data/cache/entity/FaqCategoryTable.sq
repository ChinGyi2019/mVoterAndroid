import com.popstack.mvoter2015.domain.faq.model.FaqCategoryId;

CREATE TABLE FaqCategoryTable(
    id TEXT AS FaqCategoryId PRIMARY KEY NOT NULL,
    name TEXT NOT NULL
);

insertOrReplace:
INSERT OR REPLACE INTO FaqCategoryTable VALUES (:id, :name);

selectAll:
SELECT * FROM FaqCategoryTable;

selectById:
SELECT * FROM FaqCategoryTable WHERE id = :id;